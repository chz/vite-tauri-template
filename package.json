{
  "name": "vite-tauri-template",
  "version": "1.2.0",
  "description": "Vite + Tauri + Vue 3 + Vuetify 3 desktop app template",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "vuetify",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 8080",
    "serve:native": "VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "serve:tauri": "tauri dev",
    "build:web": "vite build",
    "build": "VITE_HTTP_HANDLER=NATIVE vite build && tauri build"
  },
  "dependencies": {
    "@mdi/font": "6.4.95",
    "@tauri-apps/api": "1.0.0-beta.8",
    "roboto-fontface": "0.10.0",
    "vue": "3.2.7",
    "vue-global-api": "0.4.1",
    "vue-router": "4.0.11",
    "vuetify": "3.0.0-alpha.11",
    "ynetwork": "3.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@vitejs/plugin-vue": "1.9.4",
    "@vue/compiler-sfc": "3.2.20",
    "@vuetify/vite-plugin": "1.0.0-alpha.5",
    "sass": "1.39.0",
    "sass-loader": "12.1.0",
    "vite": "2.5.3"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/yooneskh/vite-tauri-template"
  },
  "license": "ISC"
}
